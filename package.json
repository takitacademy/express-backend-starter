{
  "name": "express-backend-starter",
  "version": "1.0.0",
  "description": "User account management ",
  "main": "src/app.ts",
  "scripts": {
    "prestart": "npm run db:migrate",
    "start": "node build/server.js",
    "prebuild": "npm run swagger",
    "build": "tsc",
    "sequelize": "./node_modules/.bin/sequelize $*",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "db:migrate": "npm exec sequelize-cli db:migrate",
    "db:migrate:undo": "npm exec sequelize-cli db:migrate:undo",
    "db:init": "npm exec sequelize-cli db:create",
    "db:seed": "npm exec sequelize-cli db:seed:all",
    "swagger": "tsoa spec",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/takitacademy/express-backend-starter.git"
  },
  "author": "frankhn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/takitacademy/express-backend-starter/issues"
  },
  "homepage": "https://github.com/takitacademy/express-backend-starter#readme",
  "dependencies": {
    "@types/cors": "^2.8.13",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "joi": "^17.9.2",
    "js-base64": "^3.7.5",
    "jsonwebtoken": "^9.0.0",
    "memory-cache-node": "^1.4.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "qs": "^6.11.2",
    "sequelize": "^6.31.1",
    "sequelize-cli": "^6.6.0",
    "sequelize-typescript": "^2.1.5",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.1",
    "tsoa": "^5.1.1",
    "typescript": "^5.3.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@types/sequelize": "^4.28.19",
    "@types/supertest": "^2.0.16",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.5.0",
    "eslint-plugin-prettier": "^5.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsconfig-paths": "^4.2.0"
  }
